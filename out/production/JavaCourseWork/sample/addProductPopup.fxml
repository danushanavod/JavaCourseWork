<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="318.0" prefWidth="479.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.ProductController">
   <children>
      <Label alignment="CENTER" layoutY="25.0" prefHeight="32.0" prefWidth="479.0" text="Add Product">
         <font>
            <Font size="22.0" />
         </font>
      </Label>
      <TextField fx:id="addProductName" layoutX="156.0" layoutY="93.0" prefHeight="25.0" prefWidth="282.0" />
      <TextField fx:id="addProductID" layoutX="156.0" layoutY="146.0" prefHeight="25.0" prefWidth="282.0" />
      <Label layoutX="32.0" layoutY="97.0" text="Enter Product Name:" />
      <Label layoutX="52.0" layoutY="150.0" text="Enter Product ID:" />
      <Button layoutX="355.0" layoutY="200.0" mnemonicParsing="false" onAction="#addProductBtn" text="Add Product" />
      <ChoiceBox fx:id="addProductChoiceBox" layoutX="156.0" layoutY="201.0" prefHeight="25.0" prefWidth="121.0" />
      <Label layoutX="54.0" layoutY="204.0" text="Select Category:" />
      <Button layoutX="225.0" layoutY="260.0" mnemonicParsing="false" onAction="#showAllProducts" text="All Products" />
   </children>
</AnchorPane>
